generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model fourbran_post {
  id         Int       @id(map: "post_pkey") @default(autoincrement())
  name       String?
  created_at DateTime  @default(now()) @db.Timestamptz(6)
  updated_at DateTime? @db.Timestamptz(6)

  @@index([name], map: "name_idx")
}
