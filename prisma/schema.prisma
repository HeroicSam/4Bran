generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model post {
  id         Int      @id(map: "post_pkey") @default(autoincrement())
  created_at DateTime @default(now()) @db.Timestamptz(6)
  image      String
  subject    String
  comment    String
  replies    Int      @default(0)
  images     Int      @default(0)

  @@map("fourbran_post")
}
